package ejercicio9;

public class Corredor extends Thread {

	private Carrera carrera;
	private int i;

	public Corredor(Carrera carrera, int i) {
		this.carrera = carrera;
		this.i = i;
	}

	@Override
	public void run() {
		try {
			carrera.relevar();
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
	}
}
